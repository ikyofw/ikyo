    "@reduxjs/toolkit": "2.9.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react-router-dom": "^5.3.3",
    "chart.js": "^4.5.0",
    "chartjs-plugin-zoom": "^2.2.0",
    "css-to-react-native": "^3.0.0",
    "file-saver": "2.0.5",
    "hot-formula-parser": "^4.0.0",
    "html-react-parser": "^5.2.6",
    "jsencrypt": "^3.2.1",
    "moment": "^2.29.4",
    "pdfjs-dist": "^2.16.105",
    "react": "19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-cookies": "^0.1.1",
    "react-dom": "19.1.1",
    "react-redux": "9.2.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "^5.0.0",
    "react-tooltip": "^5.11.2",
    "react-usestateref": "^1.0.8",
    "react-viewer": "^3.2.2",
    "react-widgets": "^5.8.6",
    "three": "^0.181.0",
