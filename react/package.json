    "utls": "^0.3.0",
    "web-vitals": "^2.1.4",
    "xlsx": "0.18.5"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "jest",
    "test1": "react-scripts test",
    "eject": "react-scripts eject",
    "prebuild": "python ../ikreact.py prebuild",
    "postbuild": "python ../ikreact.py postbuild"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "craco.config.js"
        ],
        "env": {
          "node": true,
          "browser": false
        },
        "parser": "espree",
        "parserOptions": {
          "ecmaVersion": 2020,
          "sourceType": "script"
        },
        "rules": {
          "no-undef": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^28.1.6",
    "@types/react": "19.1.13",
    "@types/react-csv": "^1.1.3",
    "@types/react-dom": "19.1.9",
    "@types/webpack-env": "^1.16.3",
    "antd": "^5.21.4",
    "browserslist": "^4.22.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-transform-css": "^4.0.1",
    "semver": "^7.7.2",
    "ts-jest": "^28.0.7",
    "typescript": "^4.9.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "dist"
    ],
    "transform": {
      "^.+\\.css$": "jest-transform-css",
      ".(ts|tsx)": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/mocks/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/mocks/fileMock.js"
    }
  }
}
